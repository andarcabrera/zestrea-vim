[alias]
  b = branch
  co = checkout
  ci = commit
  st = status
  br = branch
  amend = commit --amend -C HEAD
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  diffc = diff --cached

